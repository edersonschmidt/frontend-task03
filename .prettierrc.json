{
  "singleQuote": false,
  "semi": false,
  "trailingComma": "all",
  "tabWidth": 2,
  "importOrder": [
    "^react$",
    "<THIRD_PARTY_MODULES>",
    "^(@app)|(@components)|(@services)|(@models)/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "rules": {
    "react/jsx-props-no-spreading": "off"
  },
  "plugins": ["prettier-plugin-tailwindcss"]
}
